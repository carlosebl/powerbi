			     VAR V_VAL_ATUAL = CALCULATE([01 Vendas Valid])
			     //VALOR CONTEXTO MODIFICADO
			     VAR V_VAL_ANTERIOR = CALCULATE([01 Vendas Valid],PREVIOUSMONTH(D_CALENDARIO[Data_ref]))
			     //CALCULO
			     VAR V_CALC = DIVIDE(V_VAL_ATUAL,V_VAL_ANTERIOR)-1
			     RETURN
